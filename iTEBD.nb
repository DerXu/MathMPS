        RowBox[{"OptionValue", "[", "Type", "]"}]}], ",", "type", ",", 
       "coeffList", ",", 
       RowBox[{"spin", "=", 
        RowBox[{"OptionValue", "[", "Spin", "]"}]}], ",", 
