Cell["Set some simulation parameters", "Text",
 CellChangeTimes->{{3.4892339979539213`*^9, 3.489234002561816*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"bond", "=", "8"}], ";", " ", 
   RowBox[{"(*", " ", 
    RowBox[{
    "This", " ", "will", " ", "be", " ", "the", " ", "Bond", " ", 
